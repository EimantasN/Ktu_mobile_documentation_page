@model IEnumerable<KTU_Mobile_Data.User>

@{
    ViewData["Title"] = "Team members info";
}

<p>
    <a asp-action="Index">Home</a>
</p>

<h2>Team members info</h2>

<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Pareigos)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Fb)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Git)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Gmail)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Instragram)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Image)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pareigos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fb)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Git)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Instragram)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Image)
            </td>
        </tr>
}
    </tbody>
</table>